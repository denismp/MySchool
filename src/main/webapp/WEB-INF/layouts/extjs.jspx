<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:tiles="http://tiles.apache.org/tags-tiles"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">

<jsp:output doctype-root-element="HTML"
	doctype-system="about:legacy-compat" />

<jsp:directive.page contentType="text/html;charset=UTF-8" />

<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=8" />

<link rel="stylesheet" type="text/css"
	href="http://cdn.sencha.io/ext-4.2.3-gpl/resources/css/ext-all-debug.css" />
<link rel="stylesheet" type="text/css"
	href="http://cdn.sencha.io/ext-4.2.3-gpl/examples/shared/example.css" />
<script type="text/javascript" charset="utf-8"
	src="http://cdn.sencha.io/ext-4.2.3-gpl/ext-all-debug-w-comments.js"><!-- required for FF3 and Opera --></script>
<script type="text/javascript" charset="utf-8"
	src="http://cdn.sencha.io/ext-4.2.3-gpl/examples/shared/examples.js"><!-- required for FF3 and Opera --></script>
<script type="text/javascript" src="app.js"><!-- required for FF3 and Opera --></script>

<spring:message code="application_name" var="app_name" />
<title id="page-title"><spring:message code="welcome_h3"
		arguments="${app_name}" /></title>
</head>

<body class="tundra spring">
	<tiles:insertAttribute name="body" />
</body>
</html>
